{
  "name": "oers",
  "description": "Online Egyptian Rat Screw Game (multiplayer, web-sockets)",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "copy-types": "cp ./server/socketEvents.ts ./client/socketEvents.ts && cp ./server/types.ts ./client/types.ts",
    "start": "node dist/server/index.cjs",
    "dev:serve": "nodemon dist/server/index.cjs",
    "dev:server-build": "webpack --config webpack.server.config.cjs --watch",
    "dev:client": "webpack --config webpack.config.cjs --watch",
    "dev": "concurrently --kill-others \"npm run dev:client\" \"npm run dev:server-build\" \"npm run dev:serve\"",
    "build:server": "webpack --config webpack.server.config.cjs",
    "build:client": "webpack --config webpack.config.cjs",
    "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "lint": "eslint --ext .ts,.tsx .",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "clean": "rm -rf dist",
    "clean:full": "npm run clean && rm -rf node_modules && rm -rf package-lock.json"
  },
  "dependencies": {
    "@mantine/core": "^7.12.2",
    "@mantine/notifications": "^7.12.2",
    "@tabler/icons-react": "^3.17.0",
    "express": "^4.17.1",
    "framer-motion": "^11.5.4",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "tabler-icons-react": "^1.56.0",
    "uuid": "^8.3.2",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-syntax-jsx": "^7.24.7",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "concurrently": "^9.0.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "daisyui": "^4.12.22",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.3.3",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "ts-loader": "^9.5.1",
    "tsx": "^4.11.0",
    "typescript": "^4.9.5",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0",
    "webpack-node-externals": "^3.0.0"
  }
}
